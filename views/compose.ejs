<%- include("header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="">Title of the journal</label>
        <input type="text" name="pubTitle" autocomplete="off" class="form-control">
    </div>
    <div class="form-group">
        <label for="">Post</label>
        <textarea name="pubText" class="form-control"></textarea>
    </div>
    <button type="submit" class="btn">Publish</button>
</form>

<%- include("footer"); -%>